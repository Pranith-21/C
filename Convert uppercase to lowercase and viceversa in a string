#include<stdio.h>
#include<stdlib.h>
void main()
{
    int j,i=0;
    char a[10],b[10];
        gets(a);
    
    for(i=0;a[i]!='\0';i++);
    printf("%d",i);
    for(j=0;j<i;j++)
    {
        if(a[j]>=97 && a[j]<=122)
            a[j]=a[j]-32;
        else
            a[j]=a[j]+32;
    }
    for(j=0;j<i;j++)
       printf("%c",a[j]);
}
