#SELECTION SORT

#include<stdio.h>
#include<stdlib.h>


void selection(int b[],int n)
{  
    int i,m,j,k=0;
    int c[n];
    for(i=0;i<n;i++,k++)
    {
           m=b[i];
            for(j=0;j<n;j++)
                if(b[i]<m)
                    {
                        m=b[i];
                        b[i]=100;
                    }
    c[k]=m;
    }
    for(i=0;i<k;i++)
        printf("%d ",c[i]);
}


int main()
{   
    int n,i;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);

    selection(a,n);
    return 0;
}
